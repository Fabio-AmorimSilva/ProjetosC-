@model IEnumerable<ImobiliariaEntities.Agencia>

@{

    ViewData["Title"] = "Lista de Agencias";

}

<h1>Lista de AgÃªncias</h1>

<table class="table table-stripped table-bordered table-hover">
    <thead>
        <th>ID</th>
        <th>Nome</th>
        <th>Cidade</th>
        <th>Id Corretores</th>
        <th>Id Imoveis</th>
    </thead>
    <tbody>
        @foreach (var agencia in Model)
        {
            <tr>
                <td>@agencia.id</td>
                <td>@agencia.nome</td>
                <td>@agencia.idCorretores</td>
                <td>@agencia.idImoveis</td>              
            </tr>
        }
    </tbody>
</table>
