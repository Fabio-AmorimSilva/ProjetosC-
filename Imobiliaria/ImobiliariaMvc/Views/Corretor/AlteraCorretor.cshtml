@model ImobiliariaMvc.Models.CorretorViewModel;

@{
    ViewData["Title"] = "Altera Dados de Corretor";

}

<form class="form" method="POST"> 
    <legend>Altera Corretor</legend>
    <label>ID: </label>
        <input class="form-control" asp-for="Corretor.id" placeholder="ID..." required/>
    <label>Nome: </label>
        <input class="form-control" asp-for="Corretor.nome" placeholder="Nome..." required/>
    <label>ID Agência: </label>
        <input class="form-control" asp-for="Corretor.idAgencia" placeholder="ID Agência..." required/>
    <label>Idade: </label>
        <input class="form-control" asp-for="Corretor.idade" placeholder="Idade..." required/>
    <label>Vendas: </label>
        <input class="form-control" asp-for="Corretor.vendas" placeholder="Vendas..." required/>
        <input class="btn btn-dark" type="submit" placeholder="Enviar"/>
</form>